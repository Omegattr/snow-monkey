  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@inc2734/add-custom-event": "^0.5.0",
    "@inc2734/dispatch-custom-resize-event": "^0.4.0",
    "@inc2734/for-each-html-nodes": "^0.4.0",
    "@inc2734/smooth-scroll": "^1.0.1",
    "@wordpress/browserslist-config": "^2.7.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "browserslist": "^4.13.0",
    "cpy-cli": "^3.1.1",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-mq-optimize": "^1.1.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "start": "npm run build",
    "test": "composer test",
    "browsersync": "browser-sync start -p '127.0.0.1:8080' -f '**/*' 'resources/assets/**'",
    "cleanup:css": "rimraf resources/assets/css && mkdir -p resources/assets/css",
    "cleanup:js": "rimraf resources/assets/js && mkdir -p resources/assets/js",
    "cleanup:packages": "rimraf resources/assets/packages && mkdir -p resources/assets/packages",
    "cleanup:img": "rimraf resources/assets/img && mkdir -p resources/assets/img",
    "cleanup:translation": "rimraf resources/translation && mkdir -p resources/translation",
    "cleanup:zip": "rimraf snow-monkey.zip",
    "css:style": "node-sass resources/src/css/style.scss resources/assets/css/style.min.css && postcss -r resources/assets/css/style.min.css -c=postcss.config.js",
    "css:editor-style": "node-sass resources/src/css/editor-style.scss resources/assets/css/editor-style.min.css && postcss -r resources/assets/css/editor-style.min.css -c=postcss.config.js",
    "css:admin": "node-sass resources/src/css/admin.scss resources/assets/css/admin.min.css && postcss -r resources/assets/css/admin.min.css -c=postcss.config.js",
    "css:wpac": "node-sass resources/src/css/wpac.scss resources/assets/css/wpac.min.css && postcss -r resources/assets/css/wpac.min.css -c=postcss.config.js",
    "css:custom-widgets": "node-sass resources/src/css/custom-widgets.scss resources/assets/css/custom-widgets.min.css && postcss -r resources/assets/css/custom-widgets.min.css -c=postcss.config.js",
    "css:dependency:amp": "node-sass resources/src/css/dependency/amp/amp.scss resources/assets/css/dependency/amp/amp.min.css && postcss -r resources/assets/css/dependency/amp/amp.min.css -c=postcss.config.js",
    "css:dependency:contact-form-7": "node-sass resources/src/css/dependency/contact-form-7/wpcf7.scss resources/assets/css/dependency/contact-form-7/wpcf7.min.css && postcss -r resources/assets/css/dependency/contact-form-7/wpcf7.min.css -c=postcss.config.js",
    "css:dependency:elementor": "node-sass resources/src/css/dependency/elementor/elementor.scss resources/assets/css/dependency/elementor/elementor.min.css && postcss -r resources/assets/css/dependency/elementor/elementor.min.css -c=postcss.config.js",
    "css:dependency:woocommerce": "node-sass resources/src/css/dependency/woocommerce/woocommerce.scss resources/assets/css/dependency/woocommerce/woocommerce.min.css && postcss -r resources/assets/css/dependency/woocommerce/woocommerce.min.css -c=postcss.config.js",
    "css:dependency:snow-monkey-blocks:style": "node-sass resources/src/css/dependency/snow-monkey-blocks/style.scss resources/assets/css/dependency/snow-monkey-blocks/style.min.css && postcss -r resources/assets/css/dependency/snow-monkey-blocks/style.min.css -c=postcss.config.js",
    "css:dependency:snow-monkey-blocks:editor-style": "node-sass resources/src/css/dependency/snow-monkey-blocks/editor-style.scss resources/assets/css/dependency/snow-monkey-blocks/editor-style.min.css && postcss -r resources/assets/css/dependency/snow-monkey-blocks/editor-style.min.css -c=postcss.config.js",
    "css:dependency:snow-monkey-blocks": "npm-run-all -p css:dependency:snow-monkey-blocks:*",
    "css:dependency:snow-monkey-forms:style": "node-sass resources/src/css/dependency/snow-monkey-forms/style.scss resources/assets/css/dependency/snow-monkey-forms/style.min.css && postcss -r resources/assets/css/dependency/snow-monkey-forms/style.min.css -c=postcss.config.js",
    "css:dependency:snow-monkey-forms": "npm-run-all -p css:dependency:snow-monkey-forms:*",
    "css:dependency": "npm-run-all -p css:dependency:*",
    "css:php": "cpy '**/*.php' '../../assets/css' --cwd=resources/src/css --parents",
    "css": "npm run cleanup:css && npm-run-all -p css:*",
    "js:app": "webpack resources/src/js/app.js -o resources/assets/js/app.min.js --config webpack.config.js",
    "js:fix-adminbar": "webpack resources/src/js/fix-adminbar.js -o resources/assets/js/fix-adminbar.min.js --config webpack.config.js",
    "js:drop-nav": "webpack resources/src/js/drop-nav.js -o resources/assets/js/drop-nav.min.js --config webpack.config.js",
    "js:footer-sticky-nav": "webpack resources/src/js/footer-sticky-nav.js -o resources/assets/js/footer-sticky-nav.min.js --config webpack.config.js",
    "js:global-nav": "webpack resources/src/js/global-nav.js -o resources/assets/js/global-nav.min.js --config webpack.config.js",
    "js:sidebar-sticky-widget-area": "webpack resources/src/js/sidebar-sticky-widget-area.js -o resources/assets/js/sidebar-sticky-widget-area.min.js --config webpack.config.js",
    "js:widgets": "webpack resources/src/js/widgets.js -o resources/assets/js/widgets.min.js --config webpack.config.js",
    "js:page-top": "webpack resources/src/js/page-top.js -o resources/assets/js/page-top.min.js --config webpack.config.js",
    "js:hash-nav": "webpack resources/src/js/hash-nav.js -o resources/assets/js/hash-nav.min.js --config webpack.config.js",
    "js:smooth-scroll": "webpack resources/src/js/smooth-scroll.js -o resources/assets/js/smooth-scroll.min.js --config webpack.config.js",
    "js:fontawesome": "webpack resources/src/js/fontawesome.js -o resources/assets/js/fontawesome.min.js --config webpack.config.js",
    "js:customize-control": "webpack resources/src/js/customize-control.js -o resources/assets/js/customize-control.min.js",
    "js:customize-preview": "webpack resources/src/js/customize-preview.js -o resources/assets/js/customize-preview.min.js",
    "js:dependency:elementor:preview": "webpack resources/src/js/dependency/elementor/preview.js -o resources/assets/js/dependency/elementor/preview.min.js",
    "js:dependency:elementor": "npm-run-all -p js:dependency:elementor:*",
    "js:dependency:snow-monkey-blocks:app": "webpack resources/src/js/dependency/snow-monkey-blocks/app.js -o resources/assets/js/dependency/snow-monkey-blocks/app.min.js",
    "js:dependency:snow-monkey-blocks": "npm-run-all -p js:dependency:snow-monkey-blocks:*",
    "js:dependency": "npm-run-all -p js:dependency:*",
    "js": "npm run cleanup:js && npm-run-all -p js:*",
    "packages:fontawesome": "cpy 'fontawesome-free/js/all.min.js' '../../resources/assets/packages' --cwd=node_modules/@fortawesome --parents",
    "packages": "npm run cleanup:packages && npm-run-all packages:*",
    "img": "npm run cleanup:img && cpy '**' '../../assets/img' --cwd=resources/src/img --parents",
    "translation:name": "php bin/template-header.php",
    "translation": "npm run cleanup:translation && npm-run-all -p translation:*",
    "build": "npm run packages && npm-run-all -p js css img translation",
    "zip:pack": "composer install --no-dev && rsync -a --exclude='.*' --exclude='/translation' resources/ snow-monkey",
    "zip:archive": "zip -9 -qmr snow-monkey.zip snow-monkey",
    "zip": "npm run cleanup:zip && npm run zip:pack && npm run zip:archive"
  }
}
